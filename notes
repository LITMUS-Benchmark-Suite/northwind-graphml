employee -> reports to -> employee (Take care of null) (from employees.csv file only)
employee -> assigned -> territory (employee-territory file)
order -> contains certain quantitites of -> product (from order-details.csv file)
product-d -> supplied by -> supplier id (products.csv)
productid -> belongs to -> category id (products.csv)
orderid -> ordered by -> customer id (orders.csv)
orderid -> taken by ->employee id (orders.csv)
orderid -> shipped by -> shipper id (orders.csv)
territory -> belongs to -> region (territories.csv)
